{
  "name": "fusetester",
  "title": "fusetester",
  "summary": "Testing 'mounting' of file(s) using FUSE",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "bamfiles",
      "label": "Input BAM files",
      "class": "array:file",
      "optional": false
    },
    {
      "name": "baifiles",
      "label": "Indices for BAM files",
      "class": "array:file",
      "optional": false
    },
    {
      "name": "extracommands",
      "label": "Extra commands",
      "class": "array:string",
      "optional": true
    }
  ],
  "runSpec": {
    "interpreter": "python2.7",
    "file": "src/fusetester.py",
    "execDepends": [
      {"name": "fuse"},
      {"name": "python-llfuse"},
      {"name": "samtools"}
    ]
  }
}
